msgid ""
"Missing invoice sequence for invoice \"%(invoice)s\" on fiscalyear "
"\"%(fiscalyear)s\"."
msgstr ""

msgctxt "error:account.invoice:"
msgid "Payment lines can not be modified on a paid invoice \"%(invoice)s\""
msgstr ""

