msgid ""
"The credit account on journal \"%(journal)s\" is the same as invoice "
"\"%(invoice)s\"'s account."
msgstr ""

msgctxt "error:account.invoice:"
msgid "The credit account on journal \"%s\" is missing."
msgstr ""

