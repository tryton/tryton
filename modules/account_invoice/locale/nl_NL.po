msgstr ""

msgctxt "error:account.invoice:"
msgid "The debit account on journal \"%s\" is missing."
msgstr ""

msgctxt "error:account.invoice:"
msgid "The numbered invoice \"%s\" can not be deleted."
msgstr ""

msgctxt "error:account.invoice:"
msgid ""
