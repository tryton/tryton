<form cursor="planned_date">
    <label name="reference"/>
    <field name="reference"/>
    <label name="number"/>
    <field name="number"/>
    <label name="planned_date"/>
    <field name="planned_date"/>
    <label name="planned_start_date"/>
    <field name="planned_start_date"/>
    <label name="product"/>
    <field name="product"/>
    <label name="bom"/>
    <field name="bom"/>
    <label name="quantity"/>
    <field name="quantity"/>
    <label name="uom"/>
    <field name="uom"/>
    <notebook>
        <page string="Lines" id="lines">
            <field name="inputs" colspan="2"/>
            <field name="outputs" colspan="2"/>
        </page>
        <page string="Other Info" id="other">
            <label name="company"/>
            <field name="company"/>
            <newline/>
            <label name="warehouse"/>
            <field name="warehouse"/>
            <label name="location"/>
            <field name="location"/>
            <label name="effective_date"/>
            <field name="effective_date"/>
            <label name="effective_start_date"/>
            <field name="effective_start_date"/>
            <label name="cost"/>
            <field name="cost"/>
        </page>
    </notebook>
    <group col="4" colspan="4" id="state_buttons">
        <label name="state"/>
        <field name="state"/>
        <group col="-1" colspan="2" id="buttons">
            <button name="cancel" icon="tryton-cancel"/>
            <button name="draft"/>
            <button name="reset_bom" icon="tryton-clear"/>
            <button name="wait"/>
            <button name="assign_wizard" icon="tryton-go-next"/>
            <button name="run" icon="tryton-go-next"/>
            <button name="done" icon="tryton-go-next"/>
        </group>
    </group>
</form>
